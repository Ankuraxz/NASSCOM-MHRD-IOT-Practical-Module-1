void setup()
{
  pinMode(2, OUTPUT);
  pinMode(4, OUTPUT);
  pinMode(12,OUTPUT);
}

void loop()
{
  digitalWrite(2, HIGH);
  delay(250); // Wait for 250 millisecond(s)
  digitalWrite(2, LOW);
  delay(250); // Wait for 250 millisecond(s)
  digitalWrite(4, HIGH);
  delay(270);
  digitalWrite(4, LOW);
  delay(270);
  digitalWrite(12,HIGH);
  delay(300);
  digitalWrite(12,LOW);
  delay(300);
}
